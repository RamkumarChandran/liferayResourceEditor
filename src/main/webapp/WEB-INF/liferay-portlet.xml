<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN"
        "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>

    <portlet>

        <portlet-name>resource-editor-hook</portlet-name>

        <control-panel-entry-category>portal</control-panel-entry-category>
        <control-panel-entry-weight>0</control-panel-entry-weight>

        <instanceable>false</instanceable>
        <render-weight>1</render-weight>
        <ajaxable>false</ajaxable>
        <!--<header-portlet-css>/html/css/slider.css</header-portlet-css>-->
        <header-portlet-css>/html/bootstrap/css/bootstrap.css</header-portlet-css>
        <header-portlet-css>/html/bootstrap/css/bootstrap-responsive.css</header-portlet-css>
        <header-portlet-css>/html/css/resources.css</header-portlet-css>
        <header-portlet-css>/html/css/style.css</header-portlet-css>
        <!--Local version of jQuery-->
        <header-portlet-javascript>/html/bootstrap/js/bootstrap.js</header-portlet-javascript>
        <header-portlet-javascript>/html/bootstrap/js/bootstrap.min.js</header-portlet-javascript>
        <header-portlet-javascript>/html/js/jquery.min.js</header-portlet-javascript>
        <!--<header-portlet-javascript>/html/js/jquery.js</header-portlet-javascript>-->
        <header-portlet-javascript>/html/js/jquery.paginate.js</header-portlet-javascript>
        <header-portlet-javascript>/html/js/json2.js</header-portlet-javascript>
        <header-portlet-javascript>/html/js/resource-editor.js</header-portlet-javascript>
        <css-class-wrapper>resource-editor-hook</css-class-wrapper>

    </portlet>

</liferay-portlet-app>